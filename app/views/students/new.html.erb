<h1>Student Form</h1>
<%= form_tag students_path do%>
    <div><label>First Name:</label><%= text_field_tag :'student[first_name]' %></div><br>
    <div><label>Last Name:</label><%= text_field_tag :'student[last_name]'%></div><br>
    <%= submit_tag "Submit Student"%>
<% end %>
<%= params.inspect %>