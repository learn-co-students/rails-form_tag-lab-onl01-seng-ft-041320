<h1>Student Form</h1>

<%= form_tag students_path do %>
    <label>First Name:</label><br>
    <h1><%= text_field_tag name="student[first_name]" %></h1><br>

    <label>Last Name:</label><br>
    <h1><%= text_field_tag name="student[last_name]" %></h1><br>

    <%= submit_tag "Submit Student" %>

<% end %>